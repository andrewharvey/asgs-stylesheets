// white background (use #DBEEEE for blue ocean, or drop line for transparent)
Map { background-color: #FFFFFF; }

#structure {
  /* outline */
  line-color: #000000;
  line-width: 0.75;

  /* fill */
  polygon-fill: #FFFDD9;
}

@text_name: "[name]";
@text_face_name: "Linden Hill Regular";
@text_fill: black;
@text_placement: point;
@text_transform: uppercase;
